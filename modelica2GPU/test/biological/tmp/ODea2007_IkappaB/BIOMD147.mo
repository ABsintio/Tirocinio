
model BIOMD147 "ODea2007_IkappaB"

    function pow
        input  Real x;
        input  Real power;
        output Real y;
        algorithm
            y := x^power;
    end pow;

    parameter Real tr2a_txn_a = 0.0001848;
    parameter Real tr3a_mdeg_a = 0.0168;
    parameter Real tr1a_tsl_a = 0.2448;
    parameter Real a1_int_ai = 1.35;
    parameter Real d1_1_int_ai = 0.075;
    parameter Real a4_1_int_an = 30.0;
    parameter Real d4_1_int_an = 6e-05;
    parameter Real a4_2_int_an_n = 30.0;
    parameter Real d4_2_int_an_n = 6e-05;
    parameter Real a7_int_2ani = 11.1;
    parameter Real d1_2_int_2ani = 0.075;
    parameter Real a4_3_int_2ain = 30.0;
    parameter Real d4_3_int_2ain = 6e-05;
    parameter Real deg1_c_deg_a = 0.12;
    parameter Real deg1_n_deg_a_n = 0.12;
    parameter Real deg4_n_deg_an_n = 6e-05;
    parameter Real deg4_c_deg_an = 6e-05;
    parameter Real r1_deg_ai = 0.072;
    parameter Real r4_deg_ain = 0.36;
    parameter Real tp1a_loc_a = 0.018;
    parameter Real tp2a_loc_a = 0.012;
    parameter Real k2_a_loc_an = 0.828;
    parameter Real k2_b_loc_bn = 0.414;
    parameter Real tp1b_loc_b = 0.018;
    parameter Real tp2b_loc_b = 0.012;
    parameter Real r5_deg_bin = 0.12;
    parameter Real r2_deg_bi = 0.024;
    parameter Real deg5_c_deg_bn = 6e-05;
    parameter Real deg5_n_deg_bn_n = 6e-05;
    parameter Real deg2_n_deg_b_n = 0.18;
    parameter Real deg2_c_deg_b = 0.18;
    parameter Real a5_3_int_2bin = 30.0;
    parameter Real d5_3_int_2bin = 6e-05;
    parameter Real a8_int_2bni = 2.88;
    parameter Real d2_2_int_2bni = 0.105;
    parameter Real a5_2_int_bn_n = 30.0;
    parameter Real d5_2_int_bn_n = 6e-05;
    parameter Real a5_1_int_bn = 30.0;
    parameter Real d5_1_int_bn = 6e-05;
    parameter Real a2_int_bi = 0.36;
    parameter Real d2_1_int_bi = 0.105;
    parameter Real tr1b_tsl_b = 0.2448;
    parameter Real tr3b_mdeg_b = 0.0168;
    parameter Real tr2b_txn_b = 4.272e-05;
    parameter Real k1_2_loc_n = 5.4;
    parameter Real k1_1_loc_n = 0.0048;
    parameter Real tr2e_txn_e = 3.048e-05;
    parameter Real tr3e_mdeg_e = 0.0168;
    parameter Real tr1e_tsl_e = 0.2448;
    parameter Real a3_int_ei = 0.54;
    parameter Real d3_1_int_ei = 0.105;
    parameter Real a6_1_int_en = 30.0;
    parameter Real d6_1_int_en = 6e-05;
    parameter Real a6_2_int_en_n = 30.0;
    parameter Real d6_2_int_en_n = 6e-05;
    parameter Real a9_int_2eni = 4.2;
    parameter Real d3_2_int_2eni = 0.105;
    parameter Real a6_3_int_2ein = 30.0;
    parameter Real d6_3_int_2ein = 6e-05;
    parameter Real deg3_c_deg_e = 0.18;
    parameter Real deg3_n_deg_e_n = 0.18;
    parameter Real deg6_n_deg_en_n = 6e-05;
    parameter Real deg6_c_deg_en = 6e-05;
    parameter Real r3_deg_ei = 0.036;
    parameter Real r6_deg_ein = 0.18;
    parameter Real tp1e_loc_e = 0.018;
    parameter Real tp2e_loc_e = 0.012;
    parameter Real k2_e_loc_en = 0.414;
    parameter Real k_IKK_deg_IKK_deg = 0.0;
    parameter Real tr2a_i_itxn_a = 1.98;

    Real Total_IkBalpha(start=0.0);
    Real Total_IkBbeta(start=0.0);
    Real Total_IkBeps(start=0.0);

    Real IkBa_mRNA;
    Real IkBa_cytoplasm;
    Real IkBa_nucleus;
    Real IkBaIKK;
    Real IkBaNFkB_cytoplasm;
    Real IkBaNFkB_nucleus;
    Real IkBaIKKNFkB;
    Real NFkB_cytoplasm;
    Real IKK;
    Real NFkB_nucleus;
    Real IkBbIKK;
    Real IkBbIKKNFkB;
    Real IkBbNFkB_nucleus;
    Real IkBbNFkB_cytoplasm;
    Real IkBb_nucleus;
    Real IkBb_cytoplasm;
    Real IkBb_mRNA;
    Real IkBe_mRNA;
    Real IkBe_cytoplasm;
    Real IkBe_nucleus;
    Real IkBeNFkB_cytoplasm;
    Real IkBeNFkB_nucleus;
    Real IkBeIKKNFkB;
    Real IkBeIKK;

initial equation
    IkBa_mRNA = 0.0;
    IkBa_cytoplasm = 0.0;
    IkBa_nucleus = 0.0;
    IkBaIKK = 0.0;
    IkBaNFkB_cytoplasm = 0.06;
    IkBaNFkB_nucleus = 0.0;
    IkBaIKKNFkB = 0.0;
    NFkB_cytoplasm = 0.0;
    IKK = 0.001;
    NFkB_nucleus = 0.0;
    IkBbIKK = 0.0;
    IkBbIKKNFkB = 0.0;
    IkBbNFkB_nucleus = 0.0;
    IkBbNFkB_cytoplasm = 0.03;
    IkBb_nucleus = 0.0;
    IkBb_cytoplasm = 0.0;
    IkBb_mRNA = 0.0;
    IkBe_mRNA = 0.0;
    IkBe_cytoplasm = 0.0;
    IkBe_nucleus = 0.0;
    IkBeNFkB_cytoplasm = 0.01;
    IkBeNFkB_nucleus = 0.0;
    IkBeIKKNFkB = 0.0;
    IkBeIKK = 0.0;

equation
    Total_IkBalpha = IkBa_1.0 + IkBa_1.0 + IkBaIKK + IkBaIKKNFkB + IkBaNFkB_1.0 + IkBaNFkB_1.0;
    Total_IkBbeta = IkBb_1.0 + IkBb_1.0 + IkBbIKK + IkBbIKKNFkB + IkBbNFkB_1.0 + IkBbNFkB_1.0;
    Total_IkBeps = IkBe_1.0 + IkBe_1.0 + IkBeIKK + IkBeIKKNFkB + IkBeNFkB_1.0 + IkBeNFkB_1.0;
    der(IkBa_mRNA) = (1.0 * tr2a_txn_a) + (1.0 * tr1a_tsl_a * IkBa_mRNA) + (1.0 * tr2a_i_itxn_a * NFkB_1.0^2) - (1.0 * tr3a_mdeg_a * IkBa_mRNA) - (1.0 * tr1a_tsl_a * IkBa_mRNA);
    der(IkBa_cytoplasm) = (1.0 * tr1a_tsl_a * IkBa_mRNA) - (1.0 * (a1_int_ai * IkBa_1.0 * IKK - d1_1_int_ai * IkBaIKK)) - (1.0 * (a4_1_int_an * IkBa_1.0 * NFkB_1.0 - d4_1_int_an * IkBaNFkB_1.0)) - (1.0 * deg1_c_deg_a * IkBa_1.0) - (1.0 * tp1a_loc_a * IkBa_1.0 - 1.0 * tp2a_loc_a * IkBa_1.0);
    der(IkBa_nucleus) = (1.0 * tp1a_loc_a * IkBa_1.0 - 1.0 * tp2a_loc_a * IkBa_1.0) - (1.0 * (a4_2_int_an_n * IkBa_1.0 * NFkB_1.0 - d4_2_int_an_n * IkBaNFkB_1.0)) - (1.0 * deg1_n_deg_a_n * IkBa_1.0);
    der(IkBaIKK) = (1.0 * (a1_int_ai * IkBa_1.0 * IKK - d1_1_int_ai * IkBaIKK)) - (1.0 * (a4_3_int_2ain * IkBaIKK * NFkB_1.0 - d4_3_int_2ain * IkBaIKKNFkB)) - (1.0 * r1_deg_ai * IkBaIKK);
    der(IkBaNFkB_cytoplasm) = (1.0 * (a4_1_int_an * IkBa_1.0 * NFkB_1.0 - d4_1_int_an * IkBaNFkB_1.0)) + (1.0 * k2_a_loc_an * IkBaNFkB_1.0) - (1.0 * (a7_int_2ani * IkBaNFkB_1.0 * IKK - d1_2_int_2ani * IkBaIKKNFkB)) - (1.0 * deg4_c_deg_an * IkBaNFkB_1.0);
    der(IkBaNFkB_nucleus) = (1.0 * (a4_2_int_an_n * IkBa_1.0 * NFkB_1.0 - d4_2_int_an_n * IkBaNFkB_1.0)) - (1.0 * deg4_n_deg_an_n * IkBaNFkB_1.0) - (1.0 * k2_a_loc_an * IkBaNFkB_1.0);
    der(IkBaIKKNFkB) = (1.0 * (a7_int_2ani * IkBaNFkB_1.0 * IKK - d1_2_int_2ani * IkBaIKKNFkB)) + (1.0 * (a4_3_int_2ain * IkBaIKK * NFkB_1.0 - d4_3_int_2ain * IkBaIKKNFkB)) - (1.0 * r4_deg_ain * IkBaIKKNFkB);
    der(NFkB_cytoplasm) = (1.0 * deg4_c_deg_an * IkBaNFkB_1.0) + (1.0 * r4_deg_ain * IkBaIKKNFkB) + (1.0 * r5_deg_bin * IkBbIKKNFkB) + (1.0 * deg5_c_deg_bn * IkBbNFkB_1.0) + (1.0 * deg6_c_deg_en * IkBeNFkB_1.0) + (1.0 * r6_deg_ein * IkBeIKKNFkB) - (1.0 * (a4_1_int_an * IkBa_1.0 * NFkB_1.0 - d4_1_int_an * IkBaNFkB_1.0)) - (1.0 * (a4_3_int_2ain * IkBaIKK * NFkB_1.0 - d4_3_int_2ain * IkBaIKKNFkB)) - (1.0 * (a5_3_int_2bin * IkBbIKK * NFkB_1.0 - d5_3_int_2bin * IkBbIKKNFkB)) - (1.0 * (a5_1_int_bn * IkBb_1.0 * NFkB_1.0 - d5_1_int_bn * IkBbNFkB_1.0)) - (1.0 * k1_2_loc_n * NFkB_1.0 - 1.0 * k1_1_loc_n * NFkB_1.0) - (1.0 * (a6_1_int_en * IkBe_1.0 * NFkB_1.0 - d6_1_int_en * IkBeNFkB_1.0)) - (1.0 * (a6_3_int_2ein * IkBeIKK * NFkB_1.0 - d6_3_int_2ein * IkBeIKKNFkB));
    der(IKK) = (1.0 * r1_deg_ai * IkBaIKK) + (1.0 * r4_deg_ain * IkBaIKKNFkB) + (1.0 * r5_deg_bin * IkBbIKKNFkB) + (1.0 * r2_deg_bi * IkBbIKK) + (1.0 * r3_deg_ei * IkBeIKK) + (1.0 * r6_deg_ein * IkBeIKKNFkB) - (1.0 * (a1_int_ai * IkBa_1.0 * IKK - d1_1_int_ai * IkBaIKK)) - (1.0 * (a7_int_2ani * IkBaNFkB_1.0 * IKK - d1_2_int_2ani * IkBaIKKNFkB)) - (1.0 * (a8_int_2bni * IkBbNFkB_1.0 * IKK - d2_2_int_2bni * IkBbIKKNFkB)) - (1.0 * (a2_int_bi * IkBb_1.0 * IKK - d2_1_int_bi * IkBbIKK)) - (1.0 * (a3_int_ei * IkBe_1.0 * IKK - d3_1_int_ei * IkBeIKK)) - (1.0 * (a9_int_2eni * IkBeNFkB_1.0 * IKK - d3_2_int_2eni * IkBeIKKNFkB)) - (1.0 * k_IKK_deg_IKK_deg * IKK);
    der(NFkB_nucleus) = (1.0 * deg4_n_deg_an_n * IkBaNFkB_1.0) + (1.0 * deg5_n_deg_bn_n * IkBbNFkB_1.0) + (1.0 * k1_2_loc_n * NFkB_1.0 - 1.0 * k1_1_loc_n * NFkB_1.0) + (1.0 * deg6_n_deg_en_n * IkBeNFkB_1.0) - (1.0 * (a4_2_int_an_n * IkBa_1.0 * NFkB_1.0 - d4_2_int_an_n * IkBaNFkB_1.0)) - (1.0 * (a5_2_int_bn_n * IkBb_1.0 * NFkB_1.0 - d5_2_int_bn_n * IkBbNFkB_1.0)) - (1.0 * (a6_2_int_en_n * IkBe_1.0 * NFkB_1.0 - d6_2_int_en_n * IkBeNFkB_1.0));
    der(IkBbIKK) = (1.0 * (a2_int_bi * IkBb_1.0 * IKK - d2_1_int_bi * IkBbIKK)) - (1.0 * r2_deg_bi * IkBbIKK) - (1.0 * (a5_3_int_2bin * IkBbIKK * NFkB_1.0 - d5_3_int_2bin * IkBbIKKNFkB));
    der(IkBbIKKNFkB) = (1.0 * (a5_3_int_2bin * IkBbIKK * NFkB_1.0 - d5_3_int_2bin * IkBbIKKNFkB)) + (1.0 * (a8_int_2bni * IkBbNFkB_1.0 * IKK - d2_2_int_2bni * IkBbIKKNFkB)) - (1.0 * r5_deg_bin * IkBbIKKNFkB);
    der(IkBbNFkB_nucleus) = (1.0 * (a5_2_int_bn_n * IkBb_1.0 * NFkB_1.0 - d5_2_int_bn_n * IkBbNFkB_1.0)) - (1.0 * k2_b_loc_bn * IkBbNFkB_1.0) - (1.0 * deg5_n_deg_bn_n * IkBbNFkB_1.0);
    der(IkBbNFkB_cytoplasm) = (1.0 * k2_b_loc_bn * IkBbNFkB_1.0) + (1.0 * (a5_1_int_bn * IkBb_1.0 * NFkB_1.0 - d5_1_int_bn * IkBbNFkB_1.0)) - (1.0 * deg5_c_deg_bn * IkBbNFkB_1.0) - (1.0 * (a8_int_2bni * IkBbNFkB_1.0 * IKK - d2_2_int_2bni * IkBbIKKNFkB));
    der(IkBb_nucleus) = (1.0 * tp1b_loc_b * IkBb_1.0 - 1.0 * tp2b_loc_b * IkBb_1.0) - (1.0 * deg2_n_deg_b_n * IkBb_1.0) - (1.0 * (a5_2_int_bn_n * IkBb_1.0 * NFkB_1.0 - d5_2_int_bn_n * IkBbNFkB_1.0));
    der(IkBb_cytoplasm) = (1.0 * tr1b_tsl_b * IkBb_mRNA) - (1.0 * tp1b_loc_b * IkBb_1.0 - 1.0 * tp2b_loc_b * IkBb_1.0) - (1.0 * deg2_c_deg_b * IkBb_1.0) - (1.0 * (a5_1_int_bn * IkBb_1.0 * NFkB_1.0 - d5_1_int_bn * IkBbNFkB_1.0)) - (1.0 * (a2_int_bi * IkBb_1.0 * IKK - d2_1_int_bi * IkBbIKK));
    der(IkBb_mRNA) = (1.0 * tr1b_tsl_b * IkBb_mRNA) + (1.0 * tr2b_txn_b) - (1.0 * tr1b_tsl_b * IkBb_mRNA) - (1.0 * tr3b_mdeg_b * IkBb_mRNA);
    der(IkBe_mRNA) = (1.0 * tr2e_txn_e) + (1.0 * tr1e_tsl_e * IkBe_mRNA) - (1.0 * tr3e_mdeg_e * IkBe_mRNA) - (1.0 * tr1e_tsl_e * IkBe_mRNA);
    der(IkBe_cytoplasm) = (1.0 * tr1e_tsl_e * IkBe_mRNA) - (1.0 * (a3_int_ei * IkBe_1.0 * IKK - d3_1_int_ei * IkBeIKK)) - (1.0 * (a6_1_int_en * IkBe_1.0 * NFkB_1.0 - d6_1_int_en * IkBeNFkB_1.0)) - (1.0 * deg3_c_deg_e * IkBe_1.0) - (1.0 * tp1e_loc_e * IkBe_1.0 - 1.0 * tp2e_loc_e * IkBe_1.0);
    der(IkBe_nucleus) = (1.0 * tp1e_loc_e * IkBe_1.0 - 1.0 * tp2e_loc_e * IkBe_1.0) - (1.0 * (a6_2_int_en_n * IkBe_1.0 * NFkB_1.0 - d6_2_int_en_n * IkBeNFkB_1.0)) - (1.0 * deg3_n_deg_e_n * IkBe_1.0);
    der(IkBeNFkB_cytoplasm) = (1.0 * (a6_1_int_en * IkBe_1.0 * NFkB_1.0 - d6_1_int_en * IkBeNFkB_1.0)) + (1.0 * k2_e_loc_en * IkBeNFkB_1.0) - (1.0 * (a9_int_2eni * IkBeNFkB_1.0 * IKK - d3_2_int_2eni * IkBeIKKNFkB)) - (1.0 * deg6_c_deg_en * IkBeNFkB_1.0);
    der(IkBeNFkB_nucleus) = (1.0 * (a6_2_int_en_n * IkBe_1.0 * NFkB_1.0 - d6_2_int_en_n * IkBeNFkB_1.0)) - (1.0 * deg6_n_deg_en_n * IkBeNFkB_1.0) - (1.0 * k2_e_loc_en * IkBeNFkB_1.0);
    der(IkBeIKKNFkB) = (1.0 * (a9_int_2eni * IkBeNFkB_1.0 * IKK - d3_2_int_2eni * IkBeIKKNFkB)) + (1.0 * (a6_3_int_2ein * IkBeIKK * NFkB_1.0 - d6_3_int_2ein * IkBeIKKNFkB)) - (1.0 * r6_deg_ein * IkBeIKKNFkB);
    der(IkBeIKK) = (1.0 * (a3_int_ei * IkBe_1.0 * IKK - d3_1_int_ei * IkBeIKK)) - (1.0 * (a6_3_int_2ein * IkBeIKK * NFkB_1.0 - d6_3_int_2ein * IkBeIKKNFkB)) - (1.0 * r3_deg_ei * IkBeIKK);




end BIOMD147;
