
model BIOMD212 "Curien2009_Aspartate_Metabolism"

    function pow
        input  Real x;
        input  Real power;
        output Real y;
        algorithm
            y := x^power;
    end pow;

    parameter Real V_AA_RS = 0.43;
    parameter Real AK1_kforward_app_exp_Vak1 = 5.65;
    parameter Real AK1_kreverse_app_exp_Vak1 = 1.6;
    parameter Real AK1_Lys_Ki_app_exp_Vak1 = 550.0;
    parameter Real AK1_AdoMet_Ka_app_exp_Vak1 = 3.5;
    parameter Real AK1_nH_exp_Vak1 = 2.0;
    parameter Real AK2_kforward_app_exp_Vak2 = 3.15;
    parameter Real AK2_kreverse_app_exp_Vak2 = 0.86;
    parameter Real AK2_Lys_Ki_app_exp_Vak2 = 22.0;
    parameter Real AK2_nH_exp_Vak2 = 1.1;
    parameter Real AKI_kforward_app_exp_VakI = 0.36;
    parameter Real AKI_kreverse_app_exp_VakI = 0.15;
    parameter Real AKI_Thr_Ki_app_exp_VakI = 124.0;
    parameter Real AKI_nH_exp_VakI = 2.0;
    parameter Real AKII_kforward_app_exp_VakII = 1.35;
    parameter Real AKII_kreverse_app_exp_VakII = 0.22;
    parameter Real AKII_Thr_Ki_app_exp_VakII = 109.0;
    parameter Real AKII_nH_exp_VakII = 2.0;
    parameter Real ASADH_kforward_app_exp_Vasadh = 0.9;
    parameter Real ASADH_kreverse_app_exp_Vasadh = 0.23;
    parameter Real DHDPS1_k_app_exp_Vdhdps1 = 1.0;
    parameter Real DHDPS1_Lys_Ki_app_exp_Vdhdps1 = 10.0;
    parameter Real DHDPS1_nH_exp_Vdhdps1 = 2.0;
    parameter Real DHDPS2_k_app_exp_Vdhdps2 = 1.0;
    parameter Real DHDPS2_Lys_Ki_app_exp_Vdhdps2 = 33.0;
    parameter Real DHDPS2_nH_exp_Vdhdps2 = 2.0;
    parameter Real HSDHI_kforward_app_exp_Vhsdh1 = 0.84;
    parameter Real HSDHI_Thr_relative_residual_activity_app_exp_Vhsdh1 = 0.14;
    parameter Real HSDHI_Thr_relative_inhibition_app_exp_Vhsdh1 = 0.86;
    parameter Real HSDHI_Thr_Ki_app_exp_Vhsdh1 = 400.0;
    parameter Real HSDHII_kforward_app_exp_Vhsdh2 = 0.64;
    parameter Real HSDHII_Thr_relative_residual_activity_app_exp_Vhsdh2 = 0.25;
    parameter Real HSDHII_Thr_relative_inhibition_app_exp_Vhsdh2 = 0.75;
    parameter Real HSDHII_Thr_Ki_app_exp_Vhsdh2 = 8500.0;
    parameter Real HSK_kcat_app_exp_Vhsk = 2.8;
    parameter Real HSK_Hser_app_exp_Vhsk = 14.0;
    parameter Real TS1_kcatmin_exp_Vts1 = 0.42;
    parameter Real TS1_AdoMet_kcatmax_exp_Vts1 = 3.5;
    parameter Real TS1_AdoMet_Ka1_exp_Vts1 = 73.0;
    parameter Real TS1_nH_exp_Vts1 = 2.0;
    parameter Real TS1_Phosphate_Ki_exp_Vts1 = 1000.0;
    parameter Real TS1_AdoMEt_Km_no_AdoMet_exp_Vts1 = 250.0;
    parameter Real TS1_AdoMet_Ka2_exp_Vts1 = 0.5;
    parameter Real TS1_AdoMet_Ka3_exp_Vts1 = 1.09;
    parameter Real TS1_AdoMet_Ka4_exp_Vts1 = 140.0;
    parameter Real CGS_kcat_exp_Vcgs = 30.0;
    parameter Real CGS_Cys_Km_exp_Vcgs = 460.0;
    parameter Real CGS_Phser_Km_exp_Vcgs = 2500.0;
    parameter Real CGS_Phosphate_Ki_exp_Vcgs = 2000.0;
    parameter Real TD_k_app_exp_Vtd = 0.0124;
    parameter Real TD_Ile_Ki_no_Val_app_exp_Vtd = 30.0;
    parameter Real TD_Val_Ka1_app_exp_Vtd = 73.0;
    parameter Real TD_Val_Ka2_app_exp_Vtd = 615.0;
    parameter Real TD_nH_app_exp_Vtd = 3.0;
    parameter Real Lys_tRNAS_Lys_Km_VlysTRNA = 25.0;
    parameter Real Thr_tRNAS_Thr_Km_VthrTRNA = 100.0;
    parameter Real Ile_tRNAS_Ile_Km_VileTRNA = 20.0;
    parameter Real LKR_kcat_exp_VlysKR = 3.1;
    parameter Real LKR_Lys_Km_exp_VlysKR = 13000.0;
    parameter Real THA_kcat_exp_Vtha = 1.7;
    parameter Real THA_Thr_Km_exp_Vtha = 7100.0;
    parameter Real c1 = 1.0;

    Real V_Lys_RS(start=0.43);
    Real V_Thr_RS(start=0.43);
    Real V_Ile_RS(start=0.43);

    Real Asp;
    Real AK1;
    Real Lys;
    Real AdoMet;
    Real AspP;
    Real AK2;
    Real AKHSDHI;
    Real AKHSDHII;
    Real Thr;
    Real ASADH;
    Real ASA;
    Real DHDPS1;
    Real DHDPS2;
    Real Hser;
    Real PHser;
    Real HSK;
    Real TS1;
    Real Phosphate;
    Real Cys;
    Real CGS;
    Real Cysta;
    Real TD;
    Real Ile;
    Real Val;
    Real LysTRNA;
    Real ThrTRNA;
    Real IleTRNA;
    Real LKR;
    Real Sacc;
    Real THA;
    Real Gly;

initial equation
    Asp = 1500.0;
    AK1 = 0.25;
    Lys = 0.0;
    AdoMet = 20.0;
    AspP = 0.0;
    AK2 = 0.25;
    AKHSDHI = 0.63;
    AKHSDHII = 0.63;
    Thr = 0.0;
    ASADH = 11.6;
    ASA = 0.0;
    DHDPS1 = 1.6;
    DHDPS2 = 1.6;
    Hser = 0.0;
    PHser = 0.0;
    HSK = 4.0;
    TS1 = 7.4;
    Phosphate = 10000.0;
    Cys = 15.0;
    CGS = 0.7;
    Cysta = 0.0;
    TD = 0.36;
    Ile = 0.0;
    Val = 100.0;
    LysTRNA = 0.0;
    ThrTRNA = 0.0;
    IleTRNA = 0.0;
    LKR = 0.0;
    Sacc = 0.0;
    THA = 0.0;
    Gly = 0.0;

equation
    V_Lys_RS = V_AA_RS;
    V_Thr_RS = V_AA_RS;
    V_Ile_RS = V_AA_RS;
    der(Asp) = 0.0;
    der(AK1) = 0.0;
    der(Lys) = (c1 * DHDPS1_k_app_exp_Vdhdps1 * DHDPS1 * ASA * (1 / (1 + (Lys / DHDPS1_Lys_Ki_app_exp_Vdhdps1)^DHDPS1_nH_exp_Vdhdps1))) + (c1 * DHDPS2_k_app_exp_Vdhdps2 * DHDPS2 * ASA * (1 / (1 + (Lys / DHDPS2_Lys_Ki_app_exp_Vdhdps2)^DHDPS2_nH_exp_Vdhdps2))) - (c1 * V_Lys_RS * Lys / (Lys_tRNAS_Lys_Km_VlysTRNA + Lys)) - (c1 * LKR_kcat_exp_VlysKR * LKR * Lys / (LKR_Lys_Km_exp_VlysKR + Lys));
    der(AdoMet) = 0.0;
    der(AspP) = (c1 * AK1 * ((AK1_kforward_app_exp_Vak1 - AK1_kreverse_app_exp_Vak1 * AspP) / (1 + (Lys / (AK1_Lys_Ki_app_exp_Vak1 / (1 + AdoMet / AK1_AdoMet_Ka_app_exp_Vak1)))^AK1_nH_exp_Vak1))) + (c1 * AK2 * ((AK2_kforward_app_exp_Vak2 - AK2_kreverse_app_exp_Vak2 * AspP) / (1 + (Lys / AK2_Lys_Ki_app_exp_Vak2)^AK2_nH_exp_Vak2))) + (c1 * AKHSDHI * ((AKI_kforward_app_exp_VakI - AKI_kreverse_app_exp_VakI * AspP) / (1 + (Thr / AKI_Thr_Ki_app_exp_VakI)^AKI_nH_exp_VakI))) + (c1 * AKHSDHII * ((AKII_kforward_app_exp_VakII - AKII_kreverse_app_exp_VakII * AspP) / (1 + (Thr / AKII_Thr_Ki_app_exp_VakII)^AKII_nH_exp_VakII))) - (c1 * ASADH * (ASADH_kforward_app_exp_Vasadh * AspP - ASADH_kreverse_app_exp_Vasadh * ASA));
    der(AK2) = 0.0;
    der(AKHSDHI) = 0.0;
    der(AKHSDHII) = 0.0;
    der(Thr) = (c1 * TS1 * PHser * ((TS1_kcatmin_exp_Vts1 + TS1_AdoMet_kcatmax_exp_Vts1 * (AdoMet^TS1_nH_exp_Vts1 / TS1_AdoMet_Ka1_exp_Vts1)) / (1 + AdoMet^TS1_nH_exp_Vts1 / TS1_AdoMet_Ka1_exp_Vts1) / (TS1_AdoMEt_Km_no_AdoMet_exp_Vts1 * ((1 + AdoMet / TS1_AdoMet_Ka2_exp_Vts1) / (1 + AdoMet / TS1_AdoMet_Ka3_exp_Vts1) / (1 + AdoMet^TS1_nH_exp_Vts1 / TS1_AdoMet_Ka4_exp_Vts1)) * (1 + Phosphate / TS1_Phosphate_Ki_exp_Vts1) + PHser))) - (c1 * TD * Thr * (TD_k_app_exp_Vtd / (1 + (Ile / (TD_Ile_Ki_no_Val_app_exp_Vtd + TD_Val_Ka1_app_exp_Vtd * Val / (TD_Val_Ka2_app_exp_Vtd + Val)))^TD_nH_app_exp_Vtd))) - (c1 * V_Thr_RS * Thr / (Thr_tRNAS_Thr_Km_VthrTRNA + Thr)) - (c1 * THA_kcat_exp_Vtha * THA * Thr / (THA_Thr_Km_exp_Vtha + Thr));
    der(ASADH) = 0.0;
    der(ASA) = (c1 * ASADH * (ASADH_kforward_app_exp_Vasadh * AspP - ASADH_kreverse_app_exp_Vasadh * ASA)) - (c1 * DHDPS1_k_app_exp_Vdhdps1 * DHDPS1 * ASA * (1 / (1 + (Lys / DHDPS1_Lys_Ki_app_exp_Vdhdps1)^DHDPS1_nH_exp_Vdhdps1))) - (c1 * DHDPS2_k_app_exp_Vdhdps2 * DHDPS2 * ASA * (1 / (1 + (Lys / DHDPS2_Lys_Ki_app_exp_Vdhdps2)^DHDPS2_nH_exp_Vdhdps2))) - (c1 * HSDHI_kforward_app_exp_Vhsdh1 * AKHSDHI * ASA * (HSDHI_Thr_relative_residual_activity_app_exp_Vhsdh1 + HSDHI_Thr_relative_inhibition_app_exp_Vhsdh1 / (1 + Thr / HSDHI_Thr_Ki_app_exp_Vhsdh1))) - (c1 * HSDHII_kforward_app_exp_Vhsdh2 * AKHSDHII * ASA * (HSDHII_Thr_relative_residual_activity_app_exp_Vhsdh2 + HSDHII_Thr_relative_inhibition_app_exp_Vhsdh2 / (1 + Thr / HSDHII_Thr_Ki_app_exp_Vhsdh2)));
    der(DHDPS1) = 0.0;
    der(DHDPS2) = 0.0;
    der(Hser) = (c1 * HSDHI_kforward_app_exp_Vhsdh1 * AKHSDHI * ASA * (HSDHI_Thr_relative_residual_activity_app_exp_Vhsdh1 + HSDHI_Thr_relative_inhibition_app_exp_Vhsdh1 / (1 + Thr / HSDHI_Thr_Ki_app_exp_Vhsdh1))) + (c1 * HSDHII_kforward_app_exp_Vhsdh2 * AKHSDHII * ASA * (HSDHII_Thr_relative_residual_activity_app_exp_Vhsdh2 + HSDHII_Thr_relative_inhibition_app_exp_Vhsdh2 / (1 + Thr / HSDHII_Thr_Ki_app_exp_Vhsdh2))) - (c1 * HSK_kcat_app_exp_Vhsk * HSK * Hser / (HSK_Hser_app_exp_Vhsk + Hser));
    der(PHser) = (c1 * HSK_kcat_app_exp_Vhsk * HSK * Hser / (HSK_Hser_app_exp_Vhsk + Hser)) - (c1 * TS1 * PHser * ((TS1_kcatmin_exp_Vts1 + TS1_AdoMet_kcatmax_exp_Vts1 * (AdoMet^TS1_nH_exp_Vts1 / TS1_AdoMet_Ka1_exp_Vts1)) / (1 + AdoMet^TS1_nH_exp_Vts1 / TS1_AdoMet_Ka1_exp_Vts1) / (TS1_AdoMEt_Km_no_AdoMet_exp_Vts1 * ((1 + AdoMet / TS1_AdoMet_Ka2_exp_Vts1) / (1 + AdoMet / TS1_AdoMet_Ka3_exp_Vts1) / (1 + AdoMet^TS1_nH_exp_Vts1 / TS1_AdoMet_Ka4_exp_Vts1)) * (1 + Phosphate / TS1_Phosphate_Ki_exp_Vts1) + PHser))) - (c1 * CGS * PHser * (CGS_kcat_exp_Vcgs / (1 + CGS_Cys_Km_exp_Vcgs / Cys) / ((CGS_Phser_Km_exp_Vcgs / (1 + CGS_Cys_Km_exp_Vcgs / Cys)) * (1 + Phosphate / CGS_Phosphate_Ki_exp_Vcgs) + PHser)));
    der(HSK) = 0.0;
    der(TS1) = 0.0;
    der(Phosphate) = 0.0;
    der(Cys) = 0.0;
    der(CGS) = 0.0;
    der(Cysta) = 0.0;
    der(TD) = 0.0;
    der(Ile) = (c1 * TD * Thr * (TD_k_app_exp_Vtd / (1 + (Ile / (TD_Ile_Ki_no_Val_app_exp_Vtd + TD_Val_Ka1_app_exp_Vtd * Val / (TD_Val_Ka2_app_exp_Vtd + Val)))^TD_nH_app_exp_Vtd))) - (c1 * V_Ile_RS * Ile / (Ile_tRNAS_Ile_Km_VileTRNA + Ile));
    der(Val) = 0.0;
    der(LysTRNA) = 0.0;
    der(ThrTRNA) = 0.0;
    der(IleTRNA) = 0.0;
    der(LKR) = 0.0;
    der(Sacc) = 0.0;
    der(THA) = 0.0;
    der(Gly) = 0.0;




end BIOMD212;
