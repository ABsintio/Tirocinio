model BIOMD160 "Modelling of circadian rhythms in Drosophila incorporating the interlocked PER/TIM and VRI/PDP1 feedback loops"

    parameter Real dccpt = 15.060;
    parameter Real bccperp = 0.069;
    parameter Real ubccperp = 0.262;
    parameter Real bccpt = 51.000;
    parameter Real dperm = 0.053;
    parameter Real tlper = 36.000;
    parameter Real tcccperp = 11.000;
    parameter Real tcdvpmt = 0.028;
    parameter Real dvri = 1.226;
    parameter Real tcclkp = 1.420;
    parameter Real bcc = 2.349;
    parameter Real dclk = 0.200;
    parameter Real ubcc = 0.890;
    parameter Real bpt = 1.100;
    parameter Real ubpt = 2.930;
    parameter Real dper = 0.620;
    parameter Real tcccvrip = 16.860;
    parameter Real tcccpdpp = 9.831;
    parameter Real dvrim = 0.070;
    parameter Real dpdpm = 0.060;
    parameter Real ubccpt = 7.890;
    parameter Real tlvri = 14.680;
    parameter Real tlpdp = 1.870;
    parameter Real bccvrip = 0.100;
    parameter Real bccpdpp = 0.062;
    parameter Real ubccvrip = 0.276;
    parameter Real ubccpdpp = 0.145;
    parameter Real tcpdpclkp = 125.540;
    parameter Real dclkm = 0.643;
    parameter Real bvriclkp = 1.858;
    parameter Real bpdpclkp = 1.155;
    parameter Real ubvriclkp = 1.043;
    parameter Real ubpdpclkp = 0.952;
    parameter Real tcvriclkp = 0.053;
    parameter Real dpdp = 0.156;
    parameter Real tlclk = 35.000;
    parameter Real dcc = 0.184;
    parameter Real dpt = 0.279;
    parameter Real dtim = 0.620;
    parameter Real dtimm = 0.053;
    parameter Real tltim = 36.000;
    parameter Real bcctimp = 0.069;
    parameter Real ubcctimp = 0.262;
    parameter Real tccctimp = 11.000;
    parameter Real npt = 5.000;
    parameter Real nvri = 4.000;
    parameter Real npdp = 6.000;

    Real CC;
    Real CCPT;
    Real clkp;
    Real perp;
    Real clkm;
    Real perm;
    Real PT;
    Real vrip;
    Real vrim;
    Real VRI;
    Real pdpp;
    Real pdpm;
    Real PDP;
    Real CLK;
    Real PER;
    Real timp;
    Real timm;
    Real TIM;
    Real CYC;
    Real prcper;
    Real prcv;
    Real prcpdp;
    Real prvc;
    Real prpc;
    Real prct;

initial equation
    CC = 0.5566;
    CCPT = 0.4982;
    clkp = 0.003185;
    perp = 0.003185;
    clkm = 0.2583;
    perm = 0.2395;
    PT = 0.4014;
    vrip = 0.003185;
    vrim = 0.2571;
    VRI = 3.175;
    pdpp = 0.003185;
    pdpm = 0.3175;
    PDP = 4.1953;
    CLK = 3.6628;
    PER = 2.7527;
    timp = 0.003185;
    timm = 0.2395;
    TIM = 2.7527;
    CYC = 1.0;
    prcper = 0.041;
    prcv = 0.0585;
    prcpdp = 0.08;
    prvc = 0.489;
    prpc = 0.426;
    prct = 0.043;

equation
    der(CC) = CLK*bcc*CYC + CCPT*ubccpt - CC*PT*bccpt - CC*dcc - CC*ubcc;
    der(CCPT) = CC*PT*bccpt - CCPT*dccpt - CCPT*ubccpt;
    der(clkp) = 0.0;
    der(perp) = 0.0;
    der(clkm) = (prvc*tcvriclkp+prpc*tcpdpclkp+(1-prvc-prpc)*tcclkp)*clkp - clkm*dclkm;
    der(perm) = ((1-(1-prcper)^npt)*tcccperp+(1-prcper)^npt*tcdvpmt)*perp - perm*dperm;
    der(PT) = PER*TIM*bpt + CCPT*ubccpt - CC*PT*bccpt - PT*dpt - PT*ubpt;
    der(vrip) = 0.0;
    der(vrim) = ((1-(1-prcv)^nvri)*tcccvrip+(1-prcv)^nvri*tcdvpmt)*vrip - vrim*dvrim;
    der(VRI) = vrim*tlvri - VRI*dvri;
    der(pdpp) = 0.0;
    der(pdpm) = ((1-(1-prcpdp)^npdp)*tcccpdpp+(1-prcpdp)^npdp*tcdvpmt)*pdpp - pdpm*dpdpm;
    der(PDP) = pdpm*tlpdp - PDP*dpdp;
    der(CLK) = clkm*tlclk + CC*ubcc - CLK*bcc*CYC - CLK*dclk;
    der(PER) = perm*tlper + PT*ubpt - PT*ubpt - PER*dper;
    der(timp) = 0.0;
    der(timm) = ((1-(1-prct)^npt)*tccctimp+(1-prct)^npt*tcdvpmt)*timp - timm*dtimm;
    der(TIM) = PT*ubpt + timm*tltim - PER*TIM*bpt - TIM*dtim;
    der(CYC) = 0.0;
    der(prcper) = (1-prcper)*bccperp*CC - prcper*ubccperp;
    der(prcv) = (1-prcv)*bccvrip*CC - ubccvrip*prcv;
    der(prcpdp) = (1-prcpdp)*bccpdpp*CC - ubccpdpp*prcpdp;
    der(prvc) = (1-prvc-prpc)*bvriclkp*VRI - prvc*ubvriclkp;
    der(prpc) = (1-prvc-prpc)*bpdpclkp*PDP - prpc*ubpdpclkp;
    der(prct) = (1-prct)*bcctimp*CC - prct*ubcctimp;
    
end BIOMD160;