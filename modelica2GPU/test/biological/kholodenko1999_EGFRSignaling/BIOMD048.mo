model BIOMD048 "Quantification of short term signaling by the epidermal growth factor receptor"

    parameter Real k1f = 0.003;
    parameter Real k1b = 0.060;
    parameter Real k2f = 0.01;
    parameter Real k2b = 0.10;
    parameter Real k3f = 1.00;
    parameter Real k3b = 0.01;
    parameter Real K4 = 450.0;
    parameter Real V4 = 50.0;
    parameter Real k5f = 0.06;
    parameter Real k5b = 0.20;
    parameter Real k6f = 1.00;
    parameter Real k6b = 0.05;
    parameter Real k7f = 0.300;
    parameter Real k7b = 0.006;
    parameter Real V8 = 1.0;
    parameter Real K8 = 100.0;
    parameter Real k9f = 0.003;
    parameter Real k9b = 0.050;
    parameter Real k10f = 0.01;
    parameter Real k10b = 0.06;
    parameter Real k11f = 0.030;
    parameter Real k11b = 0.005;
    parameter Real k12f = 0.002;
    parameter Real k12b = 1.0e-4;
    parameter Real k13f = 0.09;
    parameter Real k13p = 0.60;
    parameter Real k14f = 6.00;
    parameter Real k14b = 0.06;
    parameter Real k15f = 0.300;
    parameter Real k15b = 9.0e-4;
    parameter Real V16 = 1.7;
    parameter Real K16 = 340.0;
    parameter Real k17f = 0.003;
    parameter Real k17b = 0.100;
    parameter Real k18f = 0.300;
    parameter Real k18b = 9.0e-4;
    parameter Real k19f = 0.010;
    parameter Real k19b = 0.021;
    parameter Real k20f = 0.120;
    parameter Real k20b = 2.4e-4;
    parameter Real k21f = 0.003;
    parameter Real k21b = 0.100;
    parameter Real k22f = 0.030;
    parameter Real k22b = 0.064;
    parameter Real k23f = 0.100;
    parameter Real k23b = 0.021;
    parameter Real k24f = 0.009;
    parameter Real k24b = 0.043;
    parameter Real k25f = 1.00;
    parameter Real k25b = 0.03;
    
    Real EGF "Epidermal_Growth_Factor";
    Real R "EGFR";
    Real Ra "EGF_EGFR";
    Real R2 "(EGF_EGFR)2";
    Real RP "(EGF_EGFR)2-P";
    Real PLCg "PLCg";
    Real RPLCg "(EGF_EGFR)2_PLCg";
    Real RPLCgP "(EGF_EGFR)2_PLCg-P";
    Real PLCgP "PLCg-P";
    Real Grb "Grb2";
    Real RG "(EGF_EGFR)2_Grb2";
    Real SOS "SOS";
    Real RGS "(EGF_EGFR)2_Grb2_SOS";
    Real GS "Grb2_SOS";
    Real Shc;
    Real RSh "(EGF_EGFR)2_Shc";
    Real RShP "(EGF_EGFR)2_Shc-P";
    Real ShP "Shc-P";
    Real RShG "(EGF_EGFR)2_Shc_Grb2";
    Real ShG "Shc_Grb2";
    Real RShGS "(EGF_EGFR)2_Shc_Grb2_SOS";
    Real ShGS "Shc_Grb2_SOS";
    Real PLCgl "PLCgP-I";

initial equation
    PLCgl = 0.0
    ShGS = 0.0;
    RShGS = 0.0;
    ShG = 0.0;
    RShG = 0.0;
    ShP = 0.0;
    RShP = 0.0;
    RSh = 0.0;
    Shc = 150.0;
    GS = 0.0;
    RGS = 0.0;
    SOS = 34.0;
    RG = 0.0;
    Grb = 85.0;
    PLCgP = 0.0;
    RPLCgP = 0.0;
    RPLCg = 0.0;
    PLCg = 105.0;
    RP = 0.0;
    R2 = 0.0;
    Ra = 0.0;
    R = 100.0;
    EGF = 680.0;

equation
    der(EGF) = -(k1f*R*EGF-k1b*Ra);
    der(R) = -(k1f*R*EGF-k1b*Ra);
    der(Ra) = k1f*R*EGF-k1b*Ra - 2*(k2f*Ra*Ra-k2b*R2);
    der(R2) = k2f*Ra*Ra-k2b*R2 + V4*RP/(K4+RP) - k3f*R2-k3b*RP;
    der(RP) = k3f*R2-k3b*RP + (k7f*RPLCgP-k7b*RP*PLCgP) + (k11f*RGS-k11b*RP*GS) + (k15f*RShP-k15b*ShP*RP) + (k18f*RShG-k18b*RP*ShG) + (k20f*RShGS-k20b*ShGS*RP) - V4*RP/(K4+RP) - (k5f*RP*PLCg-k5b*RPLCg) - (k9*RP*Grb-k9b*RG) - (k13*RP*Shc-k13b*RSh);
    der(PLCg) = V8*PLCgP/(K8+PLCgP) - (k5f*RP*PLCg-k5b*RPLCg);
    der(RPLCg) = k5f*RP*PLCg-k5b*RPLCg - (k6f*RPLCg-k6b*RPLCgP);
    der(RPLCgP) = k6f*RPLCg-k6b*RPLCgP - (k7f*RPLCgP-k7b*RP*PLCgP);
    der(PLCgP) = k7f*RPLCgP-k7b*RP*PLCgP - V8*PLCgP/(K8+PLCgP) - (k25f*PLCgP-k25b*PLCgl);
    der(Grb) = k12f*GS-k12b*Grb*SOS - (k9*RP*Grb-k9b*RG) - (k17f*RShP*Grb-k17b*RShG) - (k21f*ShP*Grb-k21b*ShG);
    der(RG) = k9*RP*Grb-k9b*RG - (k10*RG*SOS-k10b*RGS);
    der(SOS) = k12f*GS-k12b*Grb*SOS - (k10*RG*SOS-k10b*RGS) - (k19f*RShG*SOS-k19b*RShGS) - (k22f*ShG*SOS-k22b*ShGS);
    der(RGS) = k10*RG*SOS-k10b*RGS - (k11f*RGS-k11b*RP*GS);
    der(GS) = k11f*RGS-k11b*RP*GS + (k23f*ShGS-k23b*ShP*GS) - (k12f*GS-k12b*Grb*SOS) - (k24f*RShP*GS-k24b*RShGS);
    der(Shc) = V16*ShP/(K16+ShP) - (k13*RP*Shc-k13b*RSh);
    der(RSh) = k13*RP*Shc-k13b*RSh - (k14f*RSh-k14b*RShP);
    der(RShP) = k14f*RSh-k14b*RShP - (k15f*RShP-k15b*ShP*RP) - (k17f*RShP*Grb-k17b*RShG) - (k24f*RShP*GS-k24b*RShGS);
    der(ShP) = k15f*RShP-k15b*ShP*RP + (k23f*ShGS-k23b*ShP*GS) - V16*ShP/(K16+ShP) - (k21f*ShP*Grb-k21b*ShG);
    der(RShG) = k17f*RShP*Grb-k17b*RShG - (k18f*RShG-k18b*RP*ShG) - (k19f*RShG*SOS-k19b*RShGS);
    der(ShG) = k18f*RShG-k18b*RP*ShG + (k21f*ShP*Grb-k21b*ShG) - (k22f*ShG*SOS-k22b*ShGS);
    der(RShGS) = k19f*RShG*SOS-k19b*RShGS + (k24f*RShP*GS-k24b*RShGS) - (k20f*RShGS-k20b*ShGS*RP);
    der(ShGS) = k20f*RShGS-k20b*ShGS*RP + (k22f*ShG*SOS-k22b*ShGS) - (k23f*ShGS-k23b*ShP*GS);
    der(PLCgl) = k25f*PLCgP-k25b*PLCgl;

end BIOMD048;